[sub_resource type="GDScript" id="GDScript_6lbjv"]
script/source = "# PuzzlePiece.gd
extends Node3D

@onready var mesh: MeshInstance3D = $Mesh
@onready var area: Area3D = $Area
@onready var collision: CollisionShape3D = $Area/Collision

var piece_type: String = \"\"
var piece_color: Color = Color.WHITE

func set_shape(shape_type: String):
	piece_type = shape_type

	match shape_type:
		\"cube\":
			mesh.mesh = BoxMesh.new()
			collision.shape = BoxShape3D.new()
		\"sphere\":
			mesh.mesh = SphereMesh.new()
			collision.shape = SphereShape3D.new()
		\"capsule\":
			mesh.mesh = CapsuleMesh.new()
			collision.shape = CapsuleShape3D.new()
		\"cylinder\":
			mesh.mesh = CylinderMesh.new()
			collision.shape = CylinderShape3D.new()
		_:
			push_error(\"Unknown shape type: %s\" % shape_type)

func set_color(color: Color):
	piece_color = color

	var mat = StandardMaterial3D.new()
	mat.albedo_color = color
	mesh.material_override = mat
"

